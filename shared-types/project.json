{
  "name": "shared-types",
  "root": "shared-types",
  "sourceRoot": "shared-types/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/shared-types",
        "main": "shared-types/src/index.ts",
        "tsConfig": "shared-types/tsconfig.lib.json",
        "generatePackageJson": false,
        "assets": [
          { "glob": "README.md", "input": "shared-types", "output": "." },
          { "glob": "package.json", "input": "shared-types", "output": "." }
        ]
      }
    }
  }
}
